{% extends 'base.html.twig' %}

{% block title %}s3 buckets{% endblock %}

{% block body %}


    <div class="row">
        <div class="col-md-6">
            <h3>Symfony Finder Files</h3>
            <ol>
                {% for file in files %}
                    <li>
                        <a href="{{ file.pathname }}">
                            {{ file.basename }}
                        </a>
                    </li>
                {% endfor %}
            </ol>
        </div>

        <div class="col-md-6">
            <h3>S3 Library Call</h3>
            <ol>
                {% for result in results %}
                    <li>
                        <a href="{{ result.Key }}">
                            {{ result.Key }}
                        </a>
                    </li>
                {% endfor %}
            </ol>
        </div>
    </div>
{% endblock %}
